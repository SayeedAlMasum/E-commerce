@* Users/Index.cshtml *@
@model IEnumerable<E_commerce.Models.ApplicationUser>

@{
    ViewData["Title"] = "User List";
}

<h2>User List</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Email</th>
            <th>UserName</th>
            <th>Phone Number</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@user.UserName</td>
                <td>@user.PhoneNumber</td>
            </tr>
        }
    </tbody>
</table>
